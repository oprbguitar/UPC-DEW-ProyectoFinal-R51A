<div class="ui-panel ui-corner-all" style="width: 600px; margin: 0px auto">

<div class="ui-panel-titlebar ui-widget-header ui-corner-all">
    <span>USUARIOS</span>
</div>

<div class="ui-panel-body">
<h1>Lista de Usuarios</h1>

<table>
  <tr>
    <th>Ti persona</th>
    <th>Prenombre</th>
    <th>Ap paterno</th>
    <th>Ap materno</th>
    <th>Sexo</th>
    <th>Ti doc ident</th>
    <th>Nu doc ident</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.ti_persona %></td>
    <td><%= user.prenombre %></td>
    <td><%= user.ap_paterno %></td>
    <td><%= user.ap_materno %></td>
    <td><%= user.sexo %></td>
    <td><%= user.ti_doc_ident %></td>
    <td><%= user.nu_doc_ident %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
</div>

<!--<%= link_to 'New User', new_user_path %>-->
<div class="actions">
<%= button_to 'Nuevo', new_user_path, :class => "ui-button ui-widget ui-state-default", :method => :get %>
</div>